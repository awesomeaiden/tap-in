{
  "name": "tap-in-backend",
  "version": "0.0.1",
  "description": "Backend for tap-in application",
  "main": "dist/app.js",
  "scripts": {
    "start": "node ./dist/app.js",
    "gcp-build": "tsc -p . && cp tap_in.yaml dist",
    "build-and-start": "tsc && cp tap_in.yaml dist && cp -r share_pages dist && node dist/app.js",
    "win-build-and-start": "tsc && copy tap_in.yaml dist && xcopy src\\share_pages dist\\share_pages\\ /s /e /y && node dist/app.js",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awesomeaiden/tap-in.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/awesomeaiden/tap-in/issues"
  },
  "homepage": "https://github.com/awesomeaiden/tap-in#readme",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "axios": "^0.25.0",
    "express": "^4.17.2",
    "express-handlebars": "^6.0.2",
    "express-openapi-validator": "^4.13.5",
    "firebase-admin": "^10.0.2",
    "react": "^17.0.2",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0"
  }
